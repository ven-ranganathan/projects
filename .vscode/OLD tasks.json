        {
            "label": "GCC C: Build and Run active file",
            "type": "shell",
            "command": "gcc -Wall -Wextra -g \"${fileDirname}\\${fileBasename}\" -o \"${fileDirname}\\${fileBasenameNoExtension}\"  && \"${fileDirname}\\${fileBasenameNoExtension}\"",
            "group": "build",
            "problemMatcher": "$gcc"
        },
